<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="25%p"
    android:horizontalGap="10px"
    android:verticalGap="10px"
    android:keyHeight="12%p">

    <Row android:rowEdgeFlags="top">
        <Key android:codes="65" android:keyIcon="@drawable/icon_n1" android:horizontalGap="5%p" android:keyEdgeFlags="left" android:popupKeyboard="@xml/keyboard_popup_template"/>
        <Key android:codes="66" android:keyIcon="@drawable/icon_n2" android:popupCharacters="2" android:popupKeyboard="@xml/keyboard_popup_template"/>
        <Key android:codes="67" android:keyIcon="@drawable/icon_n3" android:popupCharacters="3" android:popupKeyboard="@xml/keyboard_popup_template"/>
        <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete"
            android:horizontalGap="2%p" android:keyWidth="18%p" android:keyEdgeFlags="right"
            android:isRepeatable="true"/>
    </Row>

    <Row>
        <Key android:codes="68" android:keyIcon="@drawable/icon_n4" android:horizontalGap="5%p" android:keyEdgeFlags="left" android:popupKeyboard="@xml/keyboard_popup_template"/>
        <Key android:codes="69" android:keyIcon="@drawable/icon_n5" android:popupCharacters="2" android:popupKeyboard="@xml/keyboard_popup_template"/>
        <Key android:codes="70" android:keyIcon="@drawable/icon_n6" android:popupCharacters="3" android:popupKeyboard="@xml/keyboard_popup_template"/>
    </Row>
    
    <Row>
        <Key android:codes="71" android:keyIcon="@drawable/icon_n7" android:horizontalGap="5%p" android:keyEdgeFlags="left" android:popupKeyboard="@xml/keyboard_popup_template"/>
        <Key android:codes="72" android:keyIcon="@drawable/icon_n8" android:popupCharacters="2" android:popupKeyboard="@xml/keyboard_popup_template"/>
        <Key android:codes="73" android:keyIcon="@drawable/icon_n9" android:popupCharacters="3" android:popupKeyboard="@xml/keyboard_popup_template"/>
        <Key android:codes="-1,-2" android:keyIcon="@drawable/sym_keyboard_shift"
            android:horizontalGap="2%p" android:keyWidth="18%p" android:isModifier="true"
                android:isSticky="true" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="74" android:keyIcon="@drawable/icon_asterisk" android:horizontalGap="5%p" android:keyEdgeFlags="left" android:popupKeyboard="@xml/keyboard_popup_template"/>
        <Key android:codes="75" android:keyIcon="@drawable/icon_n0" android:popupKeyboard="@xml/keyboard_popup_template"/>
        <Key android:codes="76" android:keyIcon="@drawable/sym_keyboard_space" android:popupCharacters="3" android:popupKeyboard="@xml/keyboard_popup_template"/>
        <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return"
            android:horizontalGap="2%p" android:keyWidth="18%p" android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>
    